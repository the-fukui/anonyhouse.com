{
  "name": "web",
  "version": "1.0.0",
  "packageManager": "yarn@3.0.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn generateValidationSchema && next build",
    "start": "next start",
    "lint": "next lint",
    "scaffold": "scaffdog generate",
    "generateValidationSchema": "typescript-json-schema ./tsconfig.json '*' --include ./{pages,components}/**/*.validation.ts --include ../shared/@types/realtimeDatabase.d.ts --include ../shared/constants/**/*.ts --ignoreErrors --required -o ./utils/validator/validationSchema.json"
  },
  "dependencies": {
    "@mantine/core": "^4.2.11",
    "@mantine/form": "^4.2.12",
    "@mantine/hooks": "^4.2.11",
    "@mantine/modals": "^4.2.12",
    "@mantine/next": "^4.2.11",
    "@mantine/notifications": "^4.2.12",
    "ajv": "^8.11.0",
    "emoji-mart": "^5.1.0",
    "firebase": "^9.8.4",
    "firebase-admin": "^11.0.0",
    "next": "^12.2.0",
    "next-react-svg": "^1.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recoil": "^0.7.4",
    "short-uuid": "^4.2.0",
    "simple-peer": "^9.11.1"
  },
  "devDependencies": {
    "@types/emoji-mart": "^3.0.9",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/simple-peer": "^9.11.4",
    "eslint": "8.18.0",
    "eslint-config-next": "12.2.0",
    "next-transpile-modules": "^9.0.0",
    "prettier-plugin-scaffdog": "^1.5.0",
    "sass": "^1.53.0",
    "scaffdog": "^1.5.0",
    "typescript": "4.7.4",
    "typescript-json-schema": "^0.53.1"
  }
}
